{
  "faker.locale": "pt_BR",
  "editor.wordWrap": "on",
  "editor.rulers": [
    80,
    100,
    120
  ],
  "editor.tabSize": 2,
  "editor.renderWhitespace": "boundary",
  "editor.formatOnSave": false,
  "editor.formatOnPaste": true,
  "editor.fontFamily": "'Consolas', 'Fira Code', 'Source Code Pro', 'Droid Sans Mono', 'monospace'",
  "editor.fontLigatures": false,
  "editor.linkedEditing": true,
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairsHorizontal": true,
  "editor.guides.bracketPairs": true,
  "editor.guides.highlightActiveIndentation": "always",
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.renderControlCharacters": true,
  "editor.minimap.enabled": true,
  "editor.lineNumbers": "on",
  "editor.smoothScrolling": true,
  "editor.renderLineHighlight": "line",
  "editor.cursorStyle": "block-outline",
  "workbench.editor.showTabs": true,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.localHistory.enabled": true,
  "workbench.sideBar.location": "right",
  "workbench.activityBar.visible": true,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.colorTheme": "One Dark Pro Darker",
  "workbench.editor.languageDetectionHints": {
    "untitledEditors": true,
    "notebookEditors": false
  },
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "window.confirmBeforeClose": "always",
  "files.watcherExclude": {
    "node_modules/*": true,
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true,
    "**/bower_components/**": true,
    "**/vendor/**": true,
    "**/platforms/**": true,
    "**/plugins/**": true,
    "**/dist/**": true,
    "**/.cache/**": true
  },
  "files.exclude": {
    "**/node_modules/**": true,
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true
  },
  "notebook.kernelProviderAssociations": [],
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "emmet.showSuggestionsAsSnippets": true,
  "javascript.preferences.quoteStyle": "single",
  "typescript.preferences.quoteStyle": "single",
  "prettier.arrowParens": "avoid",
  "prettier.singleQuote": true,
  "git.autofetch": true,
  "git.timeline.showUncommitted": true,
  "liveServer.settings.donotVerifyTags": true,
  "liveServer.settings.donotShowInfoMsg": true,
  "atomKeymap.promptV3Features": true,
  "oneDarkPro.italic": true,
  "oneDarkPro.vivid": true,
  "oneDarkPro.markdownStyle": true,
  "oneDarkPro.editorTheme": "One Dark Pro Darker",
  "material-icon-theme.hidesExplorerArrows": false,
  "tailwindCSS.includeLanguages": {
    "html": "html",
    "javascript": "javascript",
    "css": "css"
  },
  "editor.quickSuggestions": {
    "strings": true
  }
}
